{
    "swagger": "2.0",
    "info": {
        "description": "This Swagger docs for Astra Affiliate System.",
        "title": "Affiliate System API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
